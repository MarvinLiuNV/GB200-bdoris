[global]
group_reporting=1
ioengine=${ioengine}
readwrite=write
direct=1
bs=${blocksize}
size=${filesize}
iodepth=${iodepth}
norandommap=0
fallocate=none
time_based
runtime=${runtime}
ramp_time=${ramptime}
